<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:background="#ffffff"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="期数" />

                    <TextView
                        android:id="@+id/tv_sw_trainingCourse_periodNum"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/line_meeting" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="培训类型" />

                    <TextView
                        android:id="@+id/tv_sw_trainingCourse_trainType"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/line_meeting" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="开始时间" />

                    <TextView
                        android:id="@+id/tv_sw_trainingCourse_startTime"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/line_meeting" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="结算时间" />

                    <TextView
                        android:id="@+id/tv_sw_trainingCourse_endTime"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/line_meeting" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="培训人数" />

                    <TextView
                        android:id="@+id/tv_sw_trainingCourse_trainPeopNum"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/line_meeting" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="培训天数" />

                    <TextView
                        android:id="@+id/tv_sw_trainingCourse_trainingDays"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/line_meeting" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="项目联系人" />

                    <TextView
                        android:id="@+id/tv_sw_trainingCourse_respPeop"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                </RelativeLayout>

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/line_meeting" />

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" >

                    <TextView
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="上课教室" />

                    <TextView
                        android:id="@+id/tv_sw_trainingCourse_classRoom"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                </RelativeLayout>
            </LinearLayout>
    
       <TextView
                android:id="@+id/tv_accomdation_label"
                android:layout_width="fill_parent"
                android:layout_height="26dp"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:paddingLeft="20dp"
                android:text="公寓分配"
                android:textColor="#9a9a9a"
                android:textSize="16sp"
                android:visibility="gone"
                >
            </TextView>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/accomdation_layout"
                android:layout_marginTop="10dp"
                android:background="#ffffff"
                android:orientation="vertical"
                android:visibility="gone"
                 >

                <RelativeLayout
                    android:id="@+id/layout_1"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                     >

                    <TextView
                        android:id="@+id/tv_house_No1"
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />

                    <TextView
                        android:id="@+id/tv_houseNo1_people_num"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                    
                   
                </RelativeLayout>

              

                <RelativeLayout
                    android:id="@+id/layout_2"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp"
                    >

                     <View
                    android:id="@+id/layout_1_line"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentTop="true"
                    android:background="@color/line_meeting"
                    />
                    <TextView
                        android:id="@+id/tv_house_No2"
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />

                    <TextView
                        android:id="@+id/tv_houseNo2_people_num"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                  
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/layout_3"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp"
                    >
                  <View
                    android:id="@+id/layout_2_line"
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentTop="true"
                    android:background="@color/line_meeting"
                     />
                    <TextView
                        android:id="@+id/tv_house_No3"
                        style="@style/myproject_text_label"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />

                    <TextView
                        android:id="@+id/tv_houseNo3_people_num"
                        style="@style/myproject_text_value"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="20dp"
                        android:gravity="center_vertical"
                        android:text="" />
                </RelativeLayout>
                
            </LinearLayout>

            <TextView
                android:id="@+id/remark_lable"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="15dp"
                android:gravity="center_vertical"
                android:paddingLeft="20dp"
                android:text="备注"
                android:textColor="#9a9a9a"
                android:textSize="16sp" >
            </TextView>
            
             
            <TextView
                android:id="@+id/tv_sw_trainingCourse_remark"
                style="@style/myproject_text_value"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                
                android:background="#ffffff"
                android:lines="6"
                android:padding="5dp"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:text="" />
    

</LinearLayout>
