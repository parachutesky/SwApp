<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical" >
    <!-- <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        
        android:background="@color/line_meeting"
       /> -->
    <TextView android:id="@+id/tv_event"
                 android:layout_width="fill_parent"
                 android:layout_marginTop="15dp"
                 android:layout_height="60dp"
                 android:paddingLeft="10dp"
                 android:paddingRight="10dp"
                 android:text="system"
                 android:maxEms="35"
                 android:maxLines="2"
                 android:gravity="left"
                 android:ellipsize="end"
                 android:textColor="#1f1f1f"
                 android:textSize="18sp"
                 />
     <TextView android:id="@+id/tv_time"
                 android:layout_width="fill_parent"
                 android:layout_height="wrap_content"
                 android:text="system"
                 android:paddingLeft="10dp"
                 android:gravity="center_vertical"
                 android:singleLine="true"
                 android:textColor="#9a9a9a"
                 android:textSize="14sp"
                 />
       <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:background="@color/line_meeting"
         />
       <RelativeLayout android:layout_width="fill_parent"
                     android:layout_height="60dp"
                     >
       <ImageView android:id="@+id/iv_head"
                  android:layout_width="wrap_content"
                  android:layout_height="fill_parent"
                  android:layout_alignParentLeft="true"
                  android:src="@drawable/detail_icon_user_default"
                 />
        <TextView android:id="@+id/tv_username"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_toRightOf="@+id/iv_head"
                 android:layout_marginLeft="15dp"
                 android:text="system"
                 android:layout_centerVertical="true"
                 android:singleLine="true"
                 android:textColor="#9a9a9a"
                 android:textSize="16sp"
                 />
          <TextView android:id="@+id/tv_auditor"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="system"
                 android:layout_toLeftOf="@+id/tv_status"
                 android:layout_marginRight="10dp"
                 android:singleLine="true"
                 android:layout_centerVertical="true"
                 android:textColor="#9a9a9a"
                 android:textSize="16sp"
                 />
         <TextView android:id="@+id/tv_status"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="system"
                 android:layout_alignParentRight="true"
                 android:layout_marginRight="15dp"
                 android:singleLine="true"
                 android:layout_centerVertical="true"
                 android:textColor="#9a9a9a"
                 android:textSize="16sp"
                 />
       </RelativeLayout>
      <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="20dp"
        android:background="#000000"
       />
</LinearLayout>
