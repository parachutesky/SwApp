package com.jnwat.bean;

/**
 * 通讯录的bean
 * 
 * @author wangzh
 * 
 */
public class Contacts2 {
	private String Name;
	private String Department; // 系院
	private String Pinyin1; // 名字全拼
	private String Pinyin2; // 名字简拼
	private String Mobphone; // 手机
	private String Offphone; // 办公电话
	private String Email;
	private String Headurl; // 头像的路径
	private String firstLetter; // 首字母
    private String account ;//首字母
	public String getAccount() {
		return account;
	}

	public void setAccount(String account) {
		this.account = account;
	}

	private boolean isHadSeclect = false;//是否选中  false 默认没有选中

	public boolean isHadSeclect() {
		return isHadSeclect;
	}

	public void setHadSeclect(boolean isHadSeclect) {
		this.isHadSeclect = isHadSeclect;
	}

	public String getName() {
		return Name;
	}

	public void setName(String name) {
		Name = name;
	}

	public String getDepartment() {
		return Department;
	}

	public void setDepartment(String department) {
		Department = department;
	}

	public String getPinyin1() {
		return Pinyin1;
	}

	public void setPinyin1(String pinyin1) {
		Pinyin1 = pinyin1;
	}

	public String getPinyin2() {
		return Pinyin2;
	}

	public void setPinyin2(String pinyin2) {
		Pinyin2 = pinyin2;
	}

	public String getMobphone() {
		return Mobphone;
	}

	public void setMobphone(String mobphone) {
		Mobphone = mobphone;
	}

	public String getOffphone() {
		return Offphone;
	}

	public void setOffphone(String offphone) {
		Offphone = offphone;
	}

	public String getEmail() {
		return Email;
	}

	public void setEmail(String email) {
		Email = email;
	}

	public String getHeadurl() {
		return Headurl;
	}

	public void setHeadurl(String headurl) {
		Headurl = headurl;
	}

	public String getFirstLetter() {
		return firstLetter;
	}

	public void setFirstLetter(String firstLetter) {
		this.firstLetter = firstLetter;
	}

}
