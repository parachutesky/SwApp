<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        >

        <TextView
            android:id="@+id/tv_item_meetingapply_content_key"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="name"
            android:layout_centerVertical="true"
            android:textSize="@dimen/tv_17sp34" />

        <TextView
            android:id="@+id/tv_item_meetingapply_content_value"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
             android:layout_centerVertical="true"
            android:text="xingming"
            android:textColor="@color/meeting_black_dre"
            android:textSize="@dimen/tv_19sp38" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        android:background="@color/line_meeting" />

</LinearLayout>
