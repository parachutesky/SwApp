			                  android:layout_width="wrap_content"
			                  android:layout_height="fill_parent"
			                  android:layout_alignParentRight="true"
			                  android:layout_marginRight="20dp"
			                  android:gravity="center_vertical"
			                  style="@style/myproject_text_value"
			                  android:text=""/>
